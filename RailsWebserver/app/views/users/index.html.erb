<div id="content">
	
	<div id="userGroup">
		<% @repos.each do |user|%>
			<div class="user">
				<div id="<%= user.user %>">
					<div class="username"><%= user.user %></div>
						<div class="repo"><%= user.repo %></div>
				</div>
			</div>
		<%end%>
	</div>
	<div id="add">
		<input type="text"/>
		<button>Add</button>
	</div>
</div>